# 基金交易系统环境变量配置示例
# 复制此文件为 .env 并填入真实值

# 数据库配置
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/trading_plus

# Telegram Bot 配置
TELEGRAM_BOT_TOKEN=your_bot_token_here

# AI 模型配置（可选，用于分析功能）
# GLM_API_KEY=your_glm_api_key
# DEEPSEEK_API_KEY=your_deepseek_api_key
# MINIMAX_API_KEY=your_minimax_api_key

# 数据源配置（可选）
# AKSHARE_TIMEOUT=30

# 调度器配置
# SCHEDULER_NAV_SYNC_HOUR=16
# SCHEDULER_NAV_SYNC_MINUTE=30
# SCHEDULER_SETTLE_HOUR=17
# SCHEDULER_SETTLE_MINUTE=0
# SCHEDULER_ALERT_HOUR=18
# SCHEDULER_ALERT_MINUTE=0

# 日志配置
# LOG_LEVEL=INFO
